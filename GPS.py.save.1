import gps
import time
session =gps.gps("localhost",2947)
session.stream(gps.WATCH_ENABLE | gps.WATCH_NEWSTYLE)

while True:
    
    try:
	time.sleep(5)
	report =session.next()

	if report['class']== 'TPV':
	    if hasattr(report, 'time'):
		print report.time

    except KeyError:
		pass
    except KeyboardInterrupt:
		quit()
    except StopInterration:
		session =None
		print "GPS has terminated"
